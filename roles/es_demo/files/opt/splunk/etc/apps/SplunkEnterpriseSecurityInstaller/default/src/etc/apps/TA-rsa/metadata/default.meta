# Import the Splunk TA Windows Technology Addon since this TA builds on it
[]
import = Splunk_TA_windows
## shared Application-level permissions
[]
access = read : [ * ], write : [ admin ]
export = system

[savedsearches]
owner = admin

[governance]
access = read : [ * ], write : [ * ]

## Postprocess
[postprocess]
access = read : [ * ], write : [ * ]


## Exclude export of custom alert actions
[alert_actions/email]
export = none